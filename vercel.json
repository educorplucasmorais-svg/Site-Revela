{
    "buildCommand": "npm run build",
    "outputDirectory": "dist/client",
    "installCommand": "npm install",
    "framework": "vite",
    "rewrites": [
        { "source": "/api/(.*)", "destination": "https://YOUR-BACKEND-URL/api/$1" },
        { "source": "/(.*)", "destination": "/index.html" }
    ]
}